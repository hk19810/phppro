{
    "name": "my/demo-project",
    "description": "A demo PHP project for generating a dependency tree",
    "type": "project",
    "license": "MIT",
    "require": {
        "monolog/monolog": "^2.0",
        "guzzlehttp/guzzle": ""7.0.0 - 7.4.4"",
        "ramsey/uuid": "4.*",
        "symfony/console": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "DemoProject\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
 
